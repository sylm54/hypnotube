{
  "name": "Hypnotube",
  "description": "Hypnotube plugin",
  "author": "",
  "authorUrl": "",

  "sourceUrl": "https://raw.githubusercontent.com/sylm54/hypnotube/refs/heads/main/config.json",
  "repositoryUrl": "https://github.com/sylm54/hypnotube",
  "scriptUrl": "./script.js",
  "version": 27,

  "iconUrl": "./logo.png",
  "id": "bde2d2b1-0056-4053-901d-5f2861eb5b1b",

  "scriptSignature": "",
  "scriptPublicKey": "",
  "packages": ["Http", "DOMParser"],

  "allowEval": false,
  "allowUrls": ["everywhere"],
  "settings": [
    {
			"variable": "mainfeed",
			"name": "Home Feed",
			"description": "What Home Feed should be displayed",
			"type": "Dropdown",
			"default": "0",
			"options": ["Latest", "Top Rated", "Most Discussed","Most Viewed"]
		},
    {
			"variable": "mainfeed_time",
			"name": "Home Feed Time",
			"description": "Over what TimeFrame should above be aggregated",
			"type": "Dropdown",
			"default": "0",
			"options": ["All Time", "Today", "Week","Month"]
		}
  ]
}
